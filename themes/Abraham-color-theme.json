    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "name": "String interpolation",
