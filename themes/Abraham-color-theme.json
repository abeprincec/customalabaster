      "settings": {
        "foreground": "#5F9DFF"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#d680d2"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#e7e7e7"
      }
