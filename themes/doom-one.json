    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#51AFEF"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#51AFEF"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#51AFEF"
      }
    },
    {
      "scope": ["storage.modifier", "keyword.operator.noexcept"],
      "settings": {
        "foreground": "#51AFEF"
      }
    },
    {
      "scope": ["string", "meta.embedded.assembly"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
